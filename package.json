{
  "name": "@everhoof/im_everhoof",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "description": "Everhoof Instant Messaging System",
  "repository": "git@github.com:everhoof/im_everhoof.git",
  "author": {
    "name": "Svyatoslav Fyodorov",
    "email": "intelrug@gmail.com",
    "url": "https://github.com/intelrug"
  },
  "keywords": [
    "everhoof",
    "im",
    "messenger"
  ],
  "scripts": {
    "dev": "nuxt-ts",
    "build": "nuxt-ts build",
    "generate": "nuxt-ts generate",
    "start": "nuxt-ts start",
    "lint": "yarn lint:script",
    "lint:script": "eslint --ext .ts,.js,.vue --ignore-path .gitignore .",
    "format": "yarn format:script && yarn format:styles",
    "format:script": "prettier --write ./**/*.{js,ts,vue}",
    "format:styles": "stylus-supremacy format ./**/*.styl --replace --options ./stylus-supremacy.json",
    "typegen": "graphql-codegen",
    "posttypegen": "prettier --write ./graphql/**/*.ts",
    "predeploy": "yarn build",
    "deploy": "node ./scripts/deploy.js",
    "mc": "node ./scripts/make-component.js"
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "^1.0.0",
    "@nuxtjs/apollo": "^4.0.1-rc.1",
    "@nuxtjs/dotenv": "^1.4.0",
    "@nuxtjs/svg-sprite": "^0.4.10",
    "accesscontrol": "^2.2.1",
    "apollo-client": "^2.6.10",
    "apollo-link": "^1.2.14",
    "apollo-link-error": "^1.1.13",
    "axios": "^0.24.0",
    "cookie-universal-nuxt": "^2.1.4",
    "core-js": "^2.6.11",
    "luxon": "^1.25.0",
    "nuxt": "^2.14.7",
    "nuxt-i18n": "^6.9.1",
    "nuxt-property-decorator": "^2.5.1",
    "nuxt-typed-vuex": "^0.1.22",
    "popper.js": "^1.16.1",
    "typed-vuex": "^0.1.22",
    "universal-cookie": "^4.0.3",
    "vue": "^2.6.12",
    "vue-js-modal": "^2.0.0-rc.6",
    "vue-router": "^3.1.6",
    "vue2-touch-events": "^3.0.0",
    "vuex": "^3.5.1",
    "vuex-class": "^0.3.2",
    "vuex-class-component": "^2.3.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.1",
    "@babel/core": "^7.12.3",
    "@graphql-codegen/cli": "^1.17.3",
    "@graphql-codegen/schema-ast": "^1.17.3",
    "@graphql-codegen/typescript": "^1.17.3",
    "@graphql-codegen/typescript-graphql-files-modules": "^1.17.3",
    "@graphql-codegen/typescript-operations": "^1.17.3",
    "@nuxt/types": "^2.13.3",
    "@nuxt/typescript-build": "^2.0.0",
    "@nuxtjs/eslint-config-typescript": "^1.0.0",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@nuxtjs/router": "^1.5.0",
    "@nuxtjs/style-resources": "^1.0.0",
    "@types/luxon": "^1.25.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.1.4",
    "gh-pages": "^3.1.0",
    "prettier": "^2.0.5",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "stylus-supremacy": "^2.14.5",
    "typescript": "^4.0.5"
  }
}
